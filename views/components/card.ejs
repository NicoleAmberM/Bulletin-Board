<div class="card mb-3">
    <div class="card-header">
        <%= article.createdAt %>
    </div>
    <div class="card-body">
        <a href="/<%= article.id %>">
            <h5 class="card-title"><%= article.article_title %></h5>
        </a>
        <p class="card-text"><%= article.article_content %></p>
        <div class="d-flex justify-content-end">
            <a href="/edit/<%= article.id %>" class=" btn btn-warning mx-1">Edit</a>
            <a href="" class="btn btn-danger mx-1">Delete</a>
        </div>
    </div>
</div>